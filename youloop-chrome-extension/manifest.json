{
    "name": "Youtube Looper",
    "version": "1.0",
    "description": "Youtube looper",
    "content_scripts": [
        {
          "matches": ["https://*.youtube.com/*" ],
          "js": ["content.js"],
          "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "128": "images/icon.png"
      },
    "manifest_version": 2
}